{
    "name": "expectation/expectation",
    "description": "expectation library for unit testing",
    "keywords": [
        "expect", "assert", "test", "spec", "bdd"
    ],
    "require": {
        "php": ">=5.4.0",
        "doctrine/annotations": "1.2.*",
        "phpcollection/phpcollection": "0.4.*",
        "zendframework/zend-loader": "2.3.*"
    },
    "require-dev": {
        "peridot-php/peridot": "1.7.*",
        "jaz303/phake": "0.6.*",
        "beberlei/assert": "2.1.*",
        "phpspec/prophecy": "@stable",
        "cloak/cloak": "1.3.*",
        "cloak/coverallskit": "1.3.*",
        "halleck45/phpmetrics": "dev-master",
        "hoa/ruler": "dev-master"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Noritaka Horio",
            "email": "holy.shared.design@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "expectation\\": "src/"
        },
        "files": [
            "src/Autoload.php",
            "src/dsl/Function.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "expectation\\spec\\fixture\\": "spec/fixture/",
            "expectation\\spec\\reporter\\": "spec/reporter/"
        }
    },
    "scripts": {
        "test": "vendor/bin/peridot --grep *Spec.php spec",
        "metrics": "vendor/bin/phpmetrics src"
    },
    "minimum-stability": "stable"
}
