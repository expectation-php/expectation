{
    "name": "expectation/expectation",
    "description": "expectation library for unit testing",
    "version": "1.3.1",
    "keywords": [
        "expect", "assert", "test"
    ],
    "require": {
        "php": ">=5.4.0",
        "doctrine/annotations": "1.2.*",
        "phpcollection/phpcollection": "0.4.*"
    },
    "require-dev": {
        "peridot-php/peridot": "1.3.*",
        "jaz303/phake": "0.6.*",
        "beberlei/assert": "2.0.*",
        "phpspec/prophecy": "@stable",
        "cloak/cloak": "1.3.*",
        "cloak/coverallskit": "1.3.*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Noritaka Horio",
            "email": "holy.shared.design@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "expectation\\": "src/"
        },
        "files": [
            "src/Autoload.php",
            "src/dsl/Function.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "expectation\\spec\\fixture\\": "spec/fixture/",
            "expectation\\spec\\fixture\\matcher\\basic\\": "spec/fixture/matcher/basic/",
            "expectation\\spec\\fixture\\matcher\\duplicated\\": "spec/fixture/matcher/duplicated/",
            "expectation\\spec\\reporter\\": "spec/reporter/"
        }
    },
    "scripts": {
        "test": "vendor/bin/peridot --grep *Spec.php spec"
    },
    "minimum-stability": "stable"
}

