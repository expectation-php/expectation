{
    "name": "expectation/expectation",
    "description": "expectation library for unit testing",
    "keywords": [
        "expect", "assert", "test", "spec", "bdd"
    ],
    "require": {
        "php": ">=5.4.0",
        "doctrine/annotations": "1.2.*",
        "phpcollection/phpcollection": "0.4.*",
        "zendframework/zend-loader": "2.3.*",
        "noodlehaus/config": "dev-master",
        "eloquent/pathogen": "0.6.*"
    },
    "require-dev": {
        "peridot-php/peridot": "~1.8",
        "jaz303/phake": "0.6.*",
        "beberlei/assert": "2.1.*",
        "phpspec/prophecy": "@stable",
        "cloak/cloak": "1.3.*",
        "cloak/coverallskit": "1.3.*",
        "halleck45/php-metrics": "1.0.*",
        "hoa/ruler": "dev-master",
        "codegyre/robo": "0.4.*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Noritaka Horio",
            "email": "holy.shared.design@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "expectation\\": "src/"
        },
        "files": [
            "src/Autoload.php",
            "src/dsl/Function.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "expectation\\spec\\fixture\\": "spec/fixture/",
            "expectation\\spec\\reporter\\": "spec/reporter/"
        }
    },
    "scripts": {
        "test": "vendor/bin/robo spec:all",
        "coveralls": "vendor/bin/robo spec:coveralls",
        "metrics": "vendor/bin/robo php:metrics"
    },
    "minimum-stability": "stable"
}
